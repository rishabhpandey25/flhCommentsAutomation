{
  "name": "aicommentsresolvedapp",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module", 
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",            
    "server": "node dist/backend/server.js",                                 
    "client": "cd src/frontend && npm start",                                
    "build:server": "tsc -p src/backend/tsconfig.json",                      
    "build:client": "cd src/frontend && npm run build",                      
    "build": "npm run build:server && npm run build:client",                 
    "electron": "webpack && electron .",                                     
    "dev:server": "ts-node -P src/backend/tsconfig.json src/backend/server.ts",  
    "dev": "concurrently \"npm run dev:server\" \"npm run client\""
  },
